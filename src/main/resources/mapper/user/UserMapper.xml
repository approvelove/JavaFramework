<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lj.mysystem.dao.user.UserMapper">
    <select id="selectByPrimaryKey" resultType="com.lj.mysystem.entity.user.User" parameterType="java.lang.Long" >
        select *
        from sys_user
        where user_id = #{user_id,jdbcType=BIGINT}
    </select>
</mapper>